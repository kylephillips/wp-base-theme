{
    "editor_formats": {
        "container": {
            "block" : ["core/columns"],
            "label" : "Container"
        },
        "container-small": {
            "block" : ["core/columns"],
            "label" : "Container - Small"
        },
        "large-text": {
            "block" : ["core/paragraph"],
            "label" : "Large Text",
            "tinymce" : {
                "selector" : "p",
                "wrapper" : false,
                "inline" : false
            }
        },
        "small-text": {
            "block" : ["core/paragraph"],
            "label" : "Small Text",
            "tinymce" : {
                "selector" : "p",
                "wrapper" : false,
                "inline" : false
            }
        },
        "no-margin": {
            "block" : ["core/paragraph", "core/heading"],
            "label" : "No Margin",
            "tinymce" : {
                "selector" : "p,li,h1,h2,h3,h4,h5,h6",
                "wrapper" : false,
                "inline" : false
            }
        },
        "extra-large-spacer": {
            "block" : ["core/spacer"],
            "label" : "Extra Large Spacer (8em)"
        },
        "large-spacer": {
            "block" : ["core/spacer"],
            "label" : "Large Spacer (5em)"
        },
        "medium-spacer": {
            "block" : ["core/spacer"],
            "label" : "Medium Spacer (2em)"
        },
        "small-spacer": {
            "block" : ["core/spacer"],
            "label" : "Small Spacer (1em)"
        }
    },
    "image_sizes": {
        "square": {
            "label" : "Square",
            "width" : 1000,
            "height" : 1000,
            "crop" : true,
            "block_editor": true
        },
        "rectangle": {
            "label" : "Rectangle",
            "width" : 1000,
            "height" : 600,
            "crop" : true,
            "block_editor": true
        }
    }
